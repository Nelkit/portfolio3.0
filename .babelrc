{
  "presets": [
    "babel-preset-gatsby"
  ],
  "plugins": [
    [
      "prismjs",
      {
        "languages": [
          "javascript",
          "css",
          "markup",
          "swift",
          "python",
          "bash"
        ],
        "plugins": [
          "show-language",
          "line-numbers"
        ],
        "theme": "okaidia",
        "css": true
      }
    ]
  ]
}